{
    "env": {
        "es6": true,
        "node": true,
        "jquery": true,
        "mocha": true
    },
    "globals": {
  "fetch": false
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 2],
        "linebreak-style": ["error", "unix"],
        "quotes": ["error", "single"],
        "semi": ["error", "always"]
    }
}
